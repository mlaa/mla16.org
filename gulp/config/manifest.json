{
  "baseDir": "public",
  "source": ["public/**"],
  "target": "public/build",
  "options": {
    "cache": [
      "https://use.typekit.net/dfy2bmw.js"
    ],
    "exclude": [
      "robots.txt",
      "build/app.manifest",
      "build/*.map",
      "img/icons/*"
    ],
    "fallback": "/ /index.html",
    "filename": "app.manifest",
    "hash": true,
    "network": ["*"],
    "prefix": "/",
    "timestamp": false
  }
}
