{
  "baseDir": "public",
  "source": ["public/**"],
  "target": "public/build",
  "options": {
    "hash": true,
    "timestamp": false,
    "fallback": "/ /index.html",
    "prefix": "/",
    "network": ["*"],
    "filename": "app.manifest",
    "exclude": [
      "robots.txt",
      "build/app.manifest",
      "build/*.map",
      "img/icons/*"
    ]
  }
}
